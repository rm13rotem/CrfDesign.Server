@model CrfPageComponentViewModel

@{
    // property Name of CrfPageComponent

    Layout = null;
}

@try
{

    <div class="form-group">
        <label class="control-label" for="@Model.Name">@Model.QuestionText</label>
        <select class="form-control" data-val="@Model.IsRequired"
                data-val-required="This field is required." multiple
                id="@Model.Name" name="@Model.Name" value="" asp-items="@ViewData[@Model.Name] as SelectList"></select>
        <span class="text-danger field-validation-valid" data-valmsg-for="@Model.Name" data-valmsg-replace="true"></span>
    </div>
}
catch (Exception ex)
{
    <div class="form-group">
        Error in OpenSingle Field @ex.Message
    </div>
}
